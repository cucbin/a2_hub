{
  "manifest_version": 2,
  "name": "Gitlab Explorer",
  "description": "Explorer for Gitlab",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "GittTree",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "*.png",
    "*.woff2",
    "*.svg",
    "*.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [],
      "js": [
        "content.js"
      ]
    },
    {
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "css": [],
      "js": [
        "libs/contentDark.js"
      ]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}